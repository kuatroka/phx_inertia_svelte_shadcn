<!DOCTYPE html>
<html lang="en">
  <head>
    <meta charset="utf-8" />
    <meta name="viewport" content="width=device-width, initial-scale=1" />
    <meta name="csrf-token" content={get_csrf_token()} />
    <.inertia_title>{assigns[:page_title] || "Songy"}</.inertia_title>
    <.inertia_head content={assigns[:inertia_head]} />
    <PhoenixVite.Components.assets
      names={["js/app.js", "css/app.css"]}
      manifest={{:songy, "priv/static/.vite/manifest.json"}}
      dev_server={PhoenixVite.Components.has_vite_watcher?(DemoPhoenixInertiaSvelteWeb.Endpoint)}
      to_url={fn p -> static_url(@conn, p) end}
    />
  </head>
  <body>
    {@inner_content}
  </body>
</html>
